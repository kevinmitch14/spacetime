{
  "name": "spacetime",
  "version": "7.4.0",
  "description": "figure-out dates across timezones",
  "main": "src/index.js",
  "unpkg": "builds/spacetime.min.js",
  "types": "types/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./builds/spacetime-two.cjs"
    },
    "./one": {
      "import": "./src/01-one/index.js",
      "require": "./builds/spacetime-one.cjs"
    },
    "./two": {
      "import": "./src/02-two/index.js",
      "require": "./builds/spacetime-two.cjs"
    },
    "./three": {
      "import": "./src/03-three/index.js",
      "require": "./builds/spacetime-three.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "one": [
        "types/one.d.ts"
      ],
      "two": [
        "types/two.d.ts"
      ],
      "three": [
        "types/three.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rollup -c --silent",
    "build:tz": "node ./scripts/updateZonefile.js",
    "pack": "node ./scripts/pack.js",
    "version": "node ./scripts/version.js",
    "watch": "amble ./scratch.js",
    "test": "tape ./tests/**/*.test.js | tap-dancer",
    "testb": "TESTENV=prod tape ./test/**/*.test.js | tap-dancer  --color always",
    "test:one": "tape ./tests/one/**/*.test.js | tap-dancer",
    "test:types": "ts-node ./test/types/index.ts | tap-dancer  --color always",
    "coverage": "nyc -r lcov -n 'src/**/*' -n 'plugins/**/*' npm run coverage:tests",
    "coverage:tests": "npm run test",
    "codecov": "npm run coverage && codecov -t 411de6c7-82d2-41e9-a1cc-9096cdab6c72",
    "lint": "eslint ./src --ext .js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spencermountain/spacetime.git"
  },
  "files": [
    "src",
    "builds",
    "api",
    "types",
    "zonefile"
  ],
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "dependencies": {},
  "devDependencies": {
    "eslint-plugin-regexp": "1.12.0",
    "rollup": "3.9.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-filesize-check": "0.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "7.0.2",
    "shelljs": "0.8.5",
    "tap-dancer": "0.3.4",
    "tape": "5.6.1"
  },
  "license": "Apache-2.0"
}